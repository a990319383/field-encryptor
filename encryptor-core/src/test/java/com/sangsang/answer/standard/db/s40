SELECT CASE CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) WHEN CAST(AES_DECRYPT(FROM_BASE64(tr.role_name), '测试秘钥') AS CHAR) THEN 1 WHEN tr.role_desc THEN 2 WHEN 'xxx' THEN 3 ELSE 4 END, CASE WHEN tu.phone = TO_BASE64(AES_ENCRYPT(CAST(AES_DECRYPT(FROM_BASE64(tr.role_name), '测试秘钥') AS CHAR), '7uq?q8g3@q')) THEN 1 WHEN tu.phone = TO_BASE64(AES_ENCRYPT(tr.role_desc, '7uq?q8g3@q')) THEN 2 WHEN TO_BASE64(AES_ENCRYPT('xxx', '测试秘钥')) = tr.role_name THEN 3 WHEN 'yyy' = tr.role_desc THEN 4 ELSE 5 END, CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) AS phone, CAST(AES_DECRYPT(FROM_BASE64(tr.role_name), '测试秘钥') AS CHAR) AS role_name, tr.role_desc FROM tb_user tu LEFT JOIN tb_role tr ON tu.id = tr.id