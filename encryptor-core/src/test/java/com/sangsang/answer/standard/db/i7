INSERT INTO tb_user_bak (user_name, login_name, login_pwd, phone, role_id, id, create_time, update_time) (SELECT tb_user.user_name, tb_user.login_name, tb_user.login_pwd, CAST(AES_DECRYPT(FROM_BASE64(tb_user.phone), '7uq?q8g3@q') AS CHAR) AS phone, tb_user.role_id, tb_user.id, tb_user.create_time, tb_user.update_time FROM tb_user)