SELECT CAST(AES_DECRYPT(FROM_BASE64(phone), '7uq?q8g3@q') AS CHAR) AS ph, CASE tm.menu_name WHEN '1' THEN CAST(AES_DECRYPT(FROM_BASE64(phone), '7uq?q8g3@q') AS CHAR) ELSE tu.user_name END AS xxx, CASE CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) WHEN ? THEN CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) ELSE tm.menu_name END AS yyy, CASE WHEN tu.phone = TO_BASE64(AES_ENCRYPT(?, '7uq?q8g3@q')) THEN ? ELSE tm.create_time END AS zzz, tu.login_name, tm.menu_name AS menuName, tm.create_time, CAST(AES_DECRYPT(FROM_BASE64(tm.path), '7uq?q8g3@q') AS CHAR) AS path, tm.menu_name, tm.id, tm.parent_id, tm.update_time FROM tb_user tu LEFT JOIN tb_menu tm ON tu.id = tm.id