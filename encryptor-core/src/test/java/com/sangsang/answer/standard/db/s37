SELECT role_id, user_name, update_time, create_time, id, login_pwd, login_name, CAST(AES_DECRYPT(FROM_BASE64(phone), '7uq?q8g3@q') AS CHAR) AS phone FROM tb_user tu WHERE (tu.user_name, tu.phone, tu.phone) IN (SELECT CAST(AES_DECRYPT(FROM_BASE64(tr.role_name), '测试秘钥') AS CHAR) AS role_name, TO_BASE64(AES_ENCRYPT(tr.role_desc, '7uq?q8g3@q')) AS role_desc, TO_BASE64(AES_ENCRYPT(CAST(AES_DECRYPT(FROM_BASE64(tr.role_name), '测试秘钥') AS CHAR), '7uq?q8g3@q')) AS role_name FROM tb_role tr)