INSERT INTO tb_user (user_name, login_name, login_pwd, phone, role_id, id, create_time, update_time) (SELECT tb_user_bak.user_name, tb_user_bak.login_name, tb_user_bak.login_pwd, TO_BASE64(AES_ENCRYPT(tb_user_bak.phone, '7uq?q8g3@q')) AS phone, tb_user_bak.role_id, tb_user_bak.id, tb_user_bak.create_time, tb_user_bak.update_time FROM tb_user_bak)