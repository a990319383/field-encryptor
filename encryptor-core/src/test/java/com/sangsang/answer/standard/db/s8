SELECT b.ph, b.create_time AS btime FROM (SELECT menuName, login_name, ph, a.create_time FROM (SELECT CAST(AES_DECRYPT(FROM_BASE64(phone), '7uq?q8g3@q') AS CHAR) AS ph, tu.login_name, tm.menu_name AS menuName, tm.update_time, CAST(AES_DECRYPT(FROM_BASE64(tm.path), '7uq?q8g3@q') AS CHAR) AS path, tm.id, tm.menu_name, tm.create_time, tm.parent_id FROM tb_user tu LEFT JOIN tb_menu tm ON tu.id = tm.id) a WHERE ph = ?) b