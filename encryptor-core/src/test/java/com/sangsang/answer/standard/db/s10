SELECT tu.user_name, tu.id, tu.update_time, tu.role_id, tu.login_name, tu.create_time, CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) AS phone, tu.login_pwd FROM tb_user tu WHERE EXISTS (SELECT count(1) FROM tb_menu tm WHERE tm.id = tu.id AND tu.phone = TO_BASE64(AES_ENCRYPT(?, '7uq?q8g3@q')) AND tm.menu_name = CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) AND CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) LIKE '%xxx%')