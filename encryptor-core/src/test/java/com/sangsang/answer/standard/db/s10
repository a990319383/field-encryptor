SELECT role_id, user_name, update_time, create_time, id, login_pwd, login_name, CAST(AES_DECRYPT(FROM_BASE64(phone), '7uq?q8g3@q') AS CHAR) AS phone FROM tb_user tu WHERE EXISTS (SELECT count(1) FROM tb_menu tm WHERE tm.id = tu.id AND tu.phone = TO_BASE64(AES_ENCRYPT(?, '7uq?q8g3@q')) AND tm.menu_name = CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) AND CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) LIKE '%xxx%')