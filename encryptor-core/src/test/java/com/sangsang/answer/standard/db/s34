SELECT tb_user.user_name, tb_user.id, tb_user.update_time, tb_user.role_id, tb_user.login_name, tb_user.create_time, CAST(AES_DECRYPT(FROM_BASE64(tb_user.phone), '7uq?q8g3@q') AS CHAR) AS phone, tb_user.login_pwd FROM tb_user WHERE ? IN (SELECT CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) AS phone FROM tb_user tu)