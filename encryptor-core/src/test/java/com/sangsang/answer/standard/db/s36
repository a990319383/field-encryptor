SELECT role_id, user_name, update_time, create_time, id, login_pwd, login_name, CAST(AES_DECRYPT(FROM_BASE64(phone), '7uq?q8g3@q') AS CHAR) AS phone, path, menu_name, create_time, update_time, parent_id, id FROM tb_user tu LEFT JOIN (SELECT create_time, CAST(AES_DECRYPT(FROM_BASE64(path), '7uq?q8g3@q') AS CHAR) AS path, menu_name, id, parent_id, update_time FROM tb_menu tm WHERE tm.path = TO_BASE64(AES_ENCRYPT(?, '7uq?q8g3@q'))) tm_new ON CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) = tm_new.id