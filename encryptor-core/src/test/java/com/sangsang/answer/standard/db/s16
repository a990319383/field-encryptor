SELECT role_id, user_name, update_time, create_time, id, login_pwd, login_name, CAST(AES_DECRYPT(FROM_BASE64(phone), '7uq?q8g3@q') AS CHAR) AS phone, create_time, CAST(AES_DECRYPT(FROM_BASE64(path), '7uq?q8g3@q') AS CHAR) AS path, menu_name, id, parent_id, update_time FROM tb_user tu LEFT JOIN tb_menu tm ON tu.id = tm.id WHERE CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) LIKE ? AND CASE CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) WHEN ? THEN CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) LIKE ? WHEN 'xxx' THEN tm.id > ? END