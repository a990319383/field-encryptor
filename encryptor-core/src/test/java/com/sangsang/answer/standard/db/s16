SELECT tm.update_time, CAST(AES_DECRYPT(FROM_BASE64(tm.path), '7uq?q8g3@q') AS CHAR) AS path, tm.id, tm.menu_name, tm.create_time, tm.parent_id, tu.user_name, tu.id, tu.update_time, tu.role_id, tu.login_name, tu.create_time, CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) AS phone, tu.login_pwd FROM tb_user tu LEFT JOIN tb_menu tm ON tu.id = tm.id WHERE CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) LIKE ? AND CASE CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) WHEN ? THEN CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) LIKE ? WHEN 'xxx' THEN tm.id > ? END