SELECT tu.role_id, tu.user_name, tu.update_time, tu.create_time, tu.id, tu.login_pwd, tu.login_name, CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) AS phone FROM tb_user tu WHERE tu.phone IN (SELECT TO_BASE64(AES_ENCRYPT(t.user_name, '7uq?q8g3@q')) AS user_name FROM tb_user t WHERE t.phone = TO_BASE64(AES_ENCRYPT(?, '7uq?q8g3@q')))