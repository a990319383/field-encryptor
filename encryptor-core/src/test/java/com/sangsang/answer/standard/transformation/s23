SELECT CASE WHEN 2 = 0 THEN '' WHEN 2 > 0 THEN CASE WHEN INSTR(LISTAGG("user_name", ','), ',', 1, 2) = 0 THEN LISTAGG("user_name", ',') ELSE SUBSTR(LISTAGG("user_name", ','), 1, INSTR(LISTAGG("user_name", ','), ',', 1, 2) - 1) END ELSE CASE WHEN INSTR(LISTAGG("user_name", ','), ',', -1, ABS(2)) = 0 THEN LISTAGG("user_name", ',') ELSE SUBSTR(LISTAGG("user_name", ','), INSTR(LISTAGG("user_name", ','), ',', -1, ABS(2)) + LENGTH(',')) END END FROM tb_user