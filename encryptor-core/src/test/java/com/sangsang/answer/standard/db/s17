SELECT tu.user_name, tu.id, tu.update_time, tu.role_id, tu.login_name, tu.create_time, CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) AS phone, tu.login_pwd FROM tb_user tu WHERE tu.phone = TO_BASE64(AES_ENCRYPT(?, '7uq?q8g3@q')) AND TO_BASE64(AES_ENCRYPT('xxx', '7uq?q8g3@q')) = tu.phone AND tu.phone = TO_BASE64(AES_ENCRYPT(concat(?, 'yyy'), '7uq?q8g3@q')) AND tu.phone != TO_BASE64(AES_ENCRYPT(?, '7uq?q8g3@q')) AND tu.user_name = ?