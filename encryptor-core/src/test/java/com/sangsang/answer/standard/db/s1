SELECT tu.user_name, tu.id, tu.update_time, tu.role_id, tu.login_name, tu.create_time, CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) AS phone, tu.login_pwd, tm.menu_name FROM tb_user tu LEFT JOIN tb_menu tm ON tu.id = tm.id WHERE CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) LIKE ? AND tu.phone = TO_BASE64(AES_ENCRYPT(?, '7uq?q8g3@q')) AND tm.menu_name != NULL AND tm.parent_id IN (?, ?, ?) OR tm.path IS NOT NULL OR (tm.parent_id = ? AND tm.create_time IS NOT NULL)