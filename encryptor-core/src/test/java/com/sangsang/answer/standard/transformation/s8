SELECT CASE "USER_NAME" WHEN '西瓜' THEN 1 WHEN '南瓜' THEN 2 ELSE "PHONE" END AS F1, CASE WHEN "USER_NAME" = '西瓜2' THEN 1 WHEN "USER_NAME" = '南瓜2' THEN 2 ELSE "PHONE" END AS F2, CASE WHEN concat("USER_NAME", '瓜') = '西瓜' THEN DATE_FORMAT("CREATE_TIME", '%Y-%m-%d') WHEN "USER_NAME" = '南瓜' THEN "PHONE" ELSE 3 END AS F3 FROM TB_USER