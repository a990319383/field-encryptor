SELECT role_id, user_name, update_time, create_time, id, login_pwd, login_name, CAST(AES_DECRYPT(FROM_BASE64(phone), '7uq?q8g3@q') AS CHAR) AS phone FROM tb_user tu WHERE concat(CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR), 'aaa') IN (SELECT CAST(AES_DECRYPT(FROM_BASE64(t.phone), '7uq?q8g3@q') AS CHAR) AS phone FROM tb_user t WHERE t.phone = TO_BASE64(AES_ENCRYPT(?, '7uq?q8g3@q')))