SELECT tu.user_name, tu.id, tu.update_time, tu.role_id, tu.login_name, tu.create_time, CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) AS phone, tu.login_pwd FROM tb_user tu WHERE concat(CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR), 'aaa') IN (SELECT CAST(AES_DECRYPT(FROM_BASE64(t.phone), '7uq?q8g3@q') AS CHAR) AS phone FROM tb_user t WHERE t.phone = TO_BASE64(AES_ENCRYPT(?, '7uq?q8g3@q')))