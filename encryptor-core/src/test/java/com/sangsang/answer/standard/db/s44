SELECT a.*, tu.user_name, tu.login_name, tu.login_pwd, CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) AS phone, tu.role_id, tu.id, tu.create_time, tu.update_time FROM tb_user tu JOIN (SELECT tmp.* FROM (SELECT CAST(AES_DECRYPT(FROM_BASE64(tb_role.role_name), '测试秘钥') AS CHAR) AS role_name, tb_role.role_desc, tb_role.id, tb_role.create_time, tb_role.update_time FROM tb_role) tmp) a ON tu.phone = TO_BASE64(AES_ENCRYPT(a.role_name, '7uq?q8g3@q'))