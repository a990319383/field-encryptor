UPDATE tb_user tu JOIN tb_menu tm ON tu.id = tm.id SET tm.menu_name = CAST(AES_DECRYPT(FROM_BASE64(tm.`path`), '7uq?q8g3@q') AS CHAR), tu.phone = tm.path, tu.phone = TO_BASE64(AES_ENCRYPT(tm.menu_name, '7uq?q8g3@q')), tu.phone = TO_BASE64(AES_ENCRYPT(?, '7uq?q8g3@q')) WHERE CAST(AES_DECRYPT(FROM_BASE64(tu.phone), '7uq?q8g3@q') AS CHAR) LIKE ?